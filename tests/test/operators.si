# operators.si

[[ program_entry ]]
main = proc () {
    1 + 1;
    3.0 - 1.23;
    cast(f32, 2) * 4.0;
    2 * 4.0;
    6 / 4;
    10 % 2;

    i = 4 + 2 * 1;

    s = "string";
    s += i - 2;
    i -= 1;
    s -= i - 3;
    i *= 2;
    i /= 2;
    i %= 2;
    print("%", i);

    u = 1;
    b = i <= 9;
    b = i > 0;
    b = i >= 1;
    b = i == 42;
    b = i != 0;
    # b = 123 < u     warning
    print("i = %, u = %, b = %", i, u, b);
}
