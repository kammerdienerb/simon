# foo.si

prints = proc(i: s64, strings: ...*char) {

}

do_something_with_arg = proc(arg: *char) {
    prints(i=123, strings="a", "b", "c");
}

[[ program_entry ]]
main = proc(argc: s64, argv: **char) {
    loop i = 0; i < argc; i += 1 {
        arg = argv[i];
        do_something_with_arg(arg);
    }
}
